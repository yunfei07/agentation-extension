    "@vitejs/plugin-react": "^4.3.0",
    "esbuild-sass-plugin": "^3.6.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
